cur_time = datetime.datetime.now().strftime("%Y-%m-%d %H:%M")

#print (datetime.datetime(2017, 11, 28, 17, 5))
a = datetime.datetime.strptime(cur_time, "%Y-%m-%d %H:%M")
a = int(time.mktime(a .timetuple()))
print (a)
aq= datetime.datetime(2017, 11, 28, 17, 5)
ad = datetime.datetime(2017, 11, 28, 17, 6)

aq = int(time.mktime(aq.timetuple()))
ad = int(time.mktime(ad.timetuple()))
print (aq-ad)
